
{
  "name": "sss",
  "version": "1.0.0",
  "description": "A full-stack card game application",
  "scripts": {
    "postinstall": "npm install --prefix frontend && npm install --prefix backend",
    "build": "npm run build --prefix frontend",
    "start": "node backend/server/index.js",
    "dev": "concurrently \"npm:start:frontend\" \"npm:dev:backend\"",
    "start:frontend": "npm run dev --prefix frontend",
    "dev:backend": "nodemon --config backend/nodemon.json backend/server/index.js",
    "migrate": "node backend/migrate.js",
    "post-deploy": "npm install --prefix backend"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0"
  }
}
